{
  "env": {
    "browser": true,
    "es6": true,
    "node": true
  },
  "extends": [
    "eslint:recommended",
    "plugin:react/recommended",
    "plugin:jsx-a11y/recommended"
  ],
  "parser": "babel-eslint",
  "parserOptions": {
    "ecmaFeatures": {
      "experimentalObjectRestSpread": true,
      "jsx": true
    },
    "sourceType": "module"
  },
  "plugins": [
    "react",
    "jsx-a11y"
  ],
  "rules": {
    "indent": [
      "error",
      2
    ],
    "linebreak-style": [
      "error",
      "windows"
    ],
    "quotes": [
      "error",
      "single"
    ],
    "semi": [
      "error",
      "always"
    ],
    "no-console": [
      "error",
      {
        "allow": [
          "warn",
          "error"
        ]
      }
    ],
    "no-multi-spaces": [
      1
    ],
    "prefer-const": [
      "error",
      {
        "destructuring": "any",
        "ignoreReadBeforeAssign": false
      }
    ],
    "no-new-object": [
      1
    ],
    "object-shorthand": [
      "error",
      "always"
    ],
    "quote-props": [
      1,
      "as-needed"
    ],
    "no-array-constructor": [
      1
    ],
    "array-callback-return": [
      1
    ],
    "prefer-template": [
      1
    ],
    "no-eval": [
      2
    ],
    "func-style": [
      1
    ],
    "no-loop-func": [
      2
    ],
    "wrap-iife": [
      1
    ],
    "prefer-rest-params": [
      1
    ],
    "space-before-function-paren": [
      "error",
      "never"
    ],
    "template-curly-spacing": [
      "error",
      "never"
    ],
    "space-before-blocks": [
      "error",
      "always"
    ],
    "no-param-reassign": [
      1
    ],
    "prefer-spread": [
      1
    ],
    "prefer-arrow-callback": [
      "error"
    ],
    "arrow-spacing": [
      "error",
      {
        "before": true,
        "after": true
      }
    ],
    "arrow-parens": [
      "error",
      "as-needed",
      {
        "requireForBlockBody": true
      }
    ],
    "arrow-body-style": [
      "error",
      "as-needed"
    ],
    "jsx-quotes": [
      1
    ],
    "jsx-a11y/img-has-alt": [
      2,
      {
        "elements": [
          "img",
          "object",
          "area",
          "input[type=\"image\"]"
        ],
        "img": [
          "Image"
        ],
        "object": [
          "Object"
        ],
        "area": [
          "Area"
        ],
        "input[type=\"image\"]": [
          "InputImage"
        ]
      }
    ],
    "jsx-a11y/click-events-have-key-events": [
      0
    ],
    "jsx-a11y/no-static-element-interactions": [
      0
    ],
    "react/jsx-tag-spacing": [
      1,
      {
        "closingSlash": "never",
        "beforeSelfClosing": "always",
        "afterOpening": "never"
      }
    ],
    "no-process-env": [
      0
    ],
    "react/prop-types": [
      2,
      {
        "skipUndeclared": true
      }
    ],
    "react/prefer-es6-class": [
      1,
      "always"
    ],
    "react/prefer-stateless-function": [
      1
    ],
    "react/jsx-pascal-case": [
      1
    ],
    "react/jsx-closing-bracket-location": [
      1,
      "line-aligned"
    ],
    "react/jsx-uses-vars": [
      1
    ],
    "react/no-string-refs": [
      1
    ],
    "react/jsx-boolean-value": [
      1
    ],
    "react/jsx-wrap-multilines": [
      1
    ],
    "react/self-closing-comp": [
      "error",
      {
        "component": true,
        "html": true
      }
    ],
    "react/jsx-no-bind": [
      1,
      {
        "allowArrowFunctions": true
      }
    ],
    "react/require-render-return": [
      1
    ],
    "react/sort-comp": [
      1
    ],
    "react/no-is-mounted": [
      1
    ]
  }
}