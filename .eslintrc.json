{
  "env": {
    "browser": true,
    "es6": true,
    "node": true
  },
  "extends": [
    "eslint:recommended",
    "plugin:react/recommended",
    "plugin:jsx-a11y/recommended",
    "plugin:import/errors"
  ],
  "parser": "babel-eslint",
  "parserOptions": {
    "ecmaFeatures": {
      "experimentalObjectRestSpread": true,
      "jsx": true
    },
    "sourceType": "module"
  },
  "plugins": [
    "react",
    "jsx-a11y",
    "import"
  ],
  "rules": {
    "indent": [
      1,
      2
    ],
    "linebreak-style": [
      1,
      "windows"
    ],
    "quotes": [
      2,
      "single"
    ],
    "semi": [
      1,
      "always"
    ],
    "no-console": [
      "error",
      {
        "allow": [
          "warn",
          "error"
        ]
      }
    ],
    "radix": [
      1
    ],
    "no-multi-spaces": [
      1
    ],
    "prefer-const": [
      2,
      {
        "destructuring": "any",
        "ignoreReadBeforeAssign": false
      }
    ],
    "no-new-object": [
      1
    ],
    "object-shorthand": [
      2,
      "always"
    ],
    "quote-props": [
      1,
      "as-needed"
    ],
    "no-array-constructor": [
      1
    ],
    "array-callback-return": [
      1
    ],
    "prefer-template": [
      1
    ],
    "no-eval": [
      2
    ],
    "func-style": [
      1
    ],
    "no-loop-func": [
      2
    ],
    "wrap-iife": [
      1
    ],
    "prefer-rest-params": [
      1
    ],
    "space-before-function-paren": [
      2,
      "never"
    ],
    "template-curly-spacing": [
      2,
      "never"
    ],
    "space-before-blocks": [
      2,
      "always"
    ],
    "no-param-reassign": [
      1
    ],
    "prefer-spread": [
      1
    ],
    "prefer-arrow-callback": [
      2
    ],
    "arrow-spacing": [
      2,
      {
        "before": true,
        "after": true
      }
    ],
    "arrow-parens": [
      2,
      "as-needed",
      {
        "requireForBlockBody": true
      }
    ],
    "arrow-body-style": [
      2,
      "as-needed"
    ],
    "no-useless-constructor": [
      1
    ],
    "no-duplicate-imports": [
      1
    ],
    "no-iterator": [
      2
    ],
    "no-restricted-syntax": [
      1
    ],
    "no-restricted-properties": [
      1
    ],
    "no-unneeded-ternary": [
      2
    ],
    "no-case-declarations": [
      1
    ],
    "no-plusplus": [
      1,
      {
        "allowForLoopAfterthoughts": true
      }
    ],
    "eqeqeq": [
      2,
      "smart"
    ],
    "dot-notation": [
      1
    ],
    "brace-style": [
      1
    ],
    "no-whitespace-before-property": [
      2
    ],
    "spaced-comment": [
      1
    ],
    "keyword-spacing": [
      1
    ],
    "space-infix-ops": [
      1
    ],
    "padded-blocks": [
      1,
      "never"
    ],
    "space-in-parens": [
      2,
      "never"
    ],
    "array-bracket-spacing": [
      1,
      "never"
    ],
    "object-curly-spacing": [
      1,
      "always"
    ],
    "newline-per-chained-call": [
      1
    ],
    "eol-last": [
      1
    ],
    "max-len": [
      1,
      100
    ],
    "comma-dangle": [
      1,
      "always"
    ],
    "comma-style": [
      1
    ],
    "id-length": [
      2,
      {
        "min": 3,
        "properties": "never",
        "exceptions": [
          "i",
          "j",
          "a",
          "b",
          "id"
        ]
      }
    ],
    "camelcase": [
      2
    ],
    "new-cap": [
      1
    ],
    "import/no-mutable-exports": [
      2
    ],
    "import/prefer-default-export": [
      1
    ],
    "import/first": [
      2
    ],
    "import/no-webpack-loader-syntax": [
      2
    ],
    "jsx-quotes": [
      1
    ],
    "jsx-a11y/img-has-alt": [
      2,
      {
        "elements": [
          "img",
          "object",
          "area",
          "input[type=\"image\"]"
        ],
        "img": [
          "Image"
        ],
        "object": [
          "Object"
        ],
        "area": [
          "Area"
        ],
        "input[type=\"image\"]": [
          "InputImage"
        ]
      }
    ],
    "jsx-a11y/click-events-have-key-events": [
      0
    ],
    "jsx-a11y/no-static-element-interactions": [
      0
    ],
    "react/jsx-tag-spacing": [
      1,
      {
        "closingSlash": "never",
        "beforeSelfClosing": "always",
        "afterOpening": "never"
      }
    ],
    "no-process-env": [
      0
    ],
    "react/prop-types": [
      2,
      {
        "skipUndeclared": true
      }
    ],
    "react/prefer-es6-class": [
      1,
      "always"
    ],
    "react/prefer-stateless-function": [
      1
    ],
    "react/jsx-pascal-case": [
      1
    ],
    "react/jsx-closing-bracket-location": [
      1,
      "line-aligned"
    ],
    "react/jsx-uses-vars": [
      1
    ],
    "react/no-string-refs": [
      1
    ],
    "react/jsx-boolean-value": [
      1
    ],
    "react/jsx-wrap-multilines": [
      1
    ],
    "react/self-closing-comp": [
      "error",
      {
        "component": true,
        "html": true
      }
    ],
    "react/jsx-no-bind": [
      1,
      {
        "allowArrowFunctions": true
      }
    ],
    "react/require-render-return": [
      1
    ],
    "react/sort-comp": [
      1
    ],
    "react/no-is-mounted": [
      1
    ]
  }
}